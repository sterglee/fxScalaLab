<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.embed.swing.*?>
<?import javafx.scene.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1600.0" prefWidth="1600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxScalaLab.fxScalaLabController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Scala Interpreter">
              <items>
                <MenuItem fx:id="ejmlInterpreter" mnemonicParsing="false" onAction="#initEJMLInterpreter" text="EJML" />
              </items>
            </Menu>
        </menus>
         <cursor>
            <Cursor fx:constant="W_RESIZE" />
         </cursor>
      </MenuBar>
      <ScrollPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="1200.0" prefWidth="1600.0">
        <content>
          <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="0.0" minWidth="0.0" prefHeight="1200.0" prefWidth="1600.0">
               <children>
                  <TextArea fx:id="fxeditor" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onKeyPressed="#editorKeyPressed" onMouseClicked="#editorMouseClicked" prefHeight="1200.0" prefWidth="1600.0" />
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <TextArea fx:id="outputTextArea" prefHeight="200.0" prefWidth="200.0" />
   </children>
</VBox>
